mkdir sample-project
cd sample-project
git init
echo "Hello, World!" > README.md
git add README.md
git commit -m "Initial commit"
git checkout -b feature-branch1
git add .
git commit -m "Made changes in feature-branch1"
git checkout -b feature-branch2
git add .
git commit -m "Made changes in feature-branch2"
git checkout main
git merge feature-branch1
